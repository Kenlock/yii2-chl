    "description": "Yii2 extension for Chaldene Admin Template",
    "type": "yii2-extension",
    "license": "MIT",
    "authors": [
        {
            "name": "Michail Urakov",
            "email": "mikbox74@gmail.com"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "yiisoft/yii2": "~2.0.6",
        "yiisoft/yii2-bootstrap": "~2.0.0",
        "bower-asset/metismenu" : "*",
        "bower-asset/fontawesome" : "*",
